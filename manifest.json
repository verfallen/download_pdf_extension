{
  "name": "verfallen_download_pdf_extension",
  "version": "0.0.1",
  "description": "Batch download pdf files",
  "manifest_version": 3,
  "action": {
    "default_icon": "./icon.png",
    "default_title": "pdf_downloader",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content-script.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["storage", "activeTab", "downloads", "scripting"]
}
