{
  "name": "verfallen_download_pdf_extension",
  "version": "0.0.1",
  "description": "Grab pdf links and download them.",
  "manifest_version": 3,
  "action": {
    "default_icon": "./icon.png",
    "default_title": "pdf_downloader",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "downloads", "scripting"]
}
